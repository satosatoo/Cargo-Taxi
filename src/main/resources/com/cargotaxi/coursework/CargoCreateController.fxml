<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="480.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cargotaxi.coursework.CargoCreateController">
    <children>
        <Button layoutX="280.0" layoutY="402.0" mnemonicParsing="false" onAction="#saveToList" prefHeight="43.0" prefWidth="150.0" text="Save">
            <font>
                <Font name="Verdana" size="15.0" />
            </font>
        </Button>
        <Button layoutX="50.0" layoutY="402.0" mnemonicParsing="false" onAction="#switchToCargo" prefHeight="43.0" prefWidth="150.0" text="Back">
            <font>
                <Font name="Verdana" size="15.0" />
            </font>
        </Button>
        <Label layoutX="187.0" layoutY="51.0" text="Creation">
            <font>
                <Font name="Verdana" size="24.0" />
            </font>
        </Label>
        <Text layoutX="60.0" layoutY="163.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cargo name">
            <font>
                <Font name="Verdana" size="18.0" />
            </font>
        </Text>
        <Text layoutX="82.0" layoutY="220.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pick up">
            <font>
                <Font name="Verdana" size="18.0" />
            </font>
        </Text>
        <Text layoutX="78.0" layoutY="276.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Drop off">
            <font>
                <Font name="Verdana" size="18.0" />
            </font>
        </Text>
        <TextField fx:id="cargoName" layoutX="238.0" layoutY="144.0" prefHeight="25.0" prefWidth="235.0" />
        <TextField fx:id="dropOff" layoutX="238.0" layoutY="257.0" prefHeight="25.0" prefWidth="235.0" />
        <TextField fx:id="pickUp" layoutX="238.0" layoutY="201.0" prefHeight="25.0" prefWidth="235.0" />
      <Text layoutX="82.0" layoutY="331.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Weight">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Text>
      <TextField fx:id="weight" layoutX="238.0" layoutY="312.0" prefHeight="25.0" prefWidth="235.0" />
    </children>
</AnchorPane>
