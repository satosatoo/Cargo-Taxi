            <Font name="Verdana" size="15.0" />
         </font>
      </Button>
      <Label layoutX="249.0" layoutY="49.0" text="Creation">
         <font>
            <Font name="Verdana" size="24.0" />
         </font>
      </Label>
      <Text layoutX="110.0" layoutY="163.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Full name">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Text>
      <Text layoutX="89.0" layoutY="234.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone number">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Text>
      <Text layoutX="89.0" layoutY="304.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Office address">
         <font>
            <Font name="Verdana" size="18.0" />
         </font>
      </Text>
      <TextField fx:id="fullName" layoutX="277.0" layoutY="144.0" prefHeight="25.0" prefWidth="235.0" promptText="Full Name" />
      <TextField fx:id="officeAddress" layoutX="276.0" layoutY="285.0" prefHeight="25.0" prefWidth="235.0" promptText="City, Street, 123(street number)" />
      <TextField fx:id="phoneNumber" layoutX="277.0" layoutY="215.0" prefHeight="25.0" prefWidth="235.0" promptText="+380123456789" />
   </children>
</AnchorPane>
